package com.popups.pupoo.auth.security.authorization;

public class EndpointPolicy {

    private EndpointPolicy() {}

    public static final String[] PUBLIC_ENDPOINTS = {
            "/api/auth/**",
            "/actuator/health",
            "/swagger-ui/**",
            "/v3/api-docs/**"
    };

    public static final String[] ADMIN_ENDPOINTS = {
            "/api/admin/**"
    };
}
